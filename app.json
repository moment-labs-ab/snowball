{
  "expo": {
    "name": "Snowball",
    "slug": "snowball",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.momentlabs.snowball",
    "platforms": ["ios", "android"],
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/app_icons/splash-icon-light.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.momentlabs.snowball",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserNotificationUsageDescription": "Enable notifications to remind you about your habits!"
      },
      "icon": {
        "dark": "./assets/app_icons/ios-dark.png",
        "light": "./assets/app_icons/ios-light.png",
        "tinted": "./assets/app_icons/ios-tinted.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icons/android-icon.png",
        "monochromeImage": "./assets/app_icons/android-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.momentlabs.snowball"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "eb86886a-eab6-4bf6-ae7b-361c433b9cd8"
      }
    },
    "owner": "momentlabs",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/eb86886a-eab6-4bf6-ae7b-361c433b9cd8"
    }
  }
}
